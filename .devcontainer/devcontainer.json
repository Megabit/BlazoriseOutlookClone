{
  "name": ".NET 9 + gh",
  "image": "mcr.microsoft.com/dotnet/sdk:9.0",
  "features": {
    "ghcr.io/devcontainers/features/github-cli:1": {
      "version": "latest"
    }
  },
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-dotnettools.csdevkit",
        "GitHub.copilot",
        "GitHub.copilot-chat"
      ]
    }
  },
 "postCreateCommand": "dotnet restore && dotnet dev-certs https"
}